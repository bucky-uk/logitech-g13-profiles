    <script>function OnEvent(event, arg, family)
	if event == "PROFILE_ACTIVATED" then
		SetBacklightColor(255,80,0,"kb")
		SetBacklightColor(255,80,0,"lhc")
	end

	if event == "G_PRESSED" and arg == 15 then
	    PressKey("lshift", "5")
	    Sleep(25)
	    ReleaseKey("lshift", "5")
	end

	if event == "G_PRESSED" and arg == 20 then
	    PressKey("lshift", "6")
	    Sleep(25)
	    ReleaseKey("lshift", "6")
	end

	if event == "G_RELEASED" and arg == 15 then
	    PressKey("lshift", "1")
	    Sleep(25)
	    ReleaseKey("lshift", "1")
	end

	if event == "G_RELEASED" and arg == 20 then
	    PressKey("lshift", "1")
	    Sleep(25)
	    ReleaseKey("lshift", "1")
	end

end</script>
