    <script>function OnEvent(event, arg, family)
	if event == "PROFILE_ACTIVATED" then
		SetBacklightColor(255,80,0,"kb")
		SetBacklightColor(255,80,0,"lhc")
	end

	if event == "G_PRESSED" and arg == xx15 then
	    PressKey("ralt", "num6")
	    Sleep(5)
	    ReleaseKey("ralt", "num6")
	    Sleep(5)
	    PressKey("lshift")
	end

	if event == "G_PRESSED" and arg == xx20 then
	    PressKey("ralt", "num5")
	    Sleep(5)
	    ReleaseKey("ralt", "num5")
	    Sleep(5)
	    PressKey("lctrl")
	end

	if event == "G_RELEASED" and arg == xx15 then
	    ReleaseKey("lshift")
	    Sleep(5)
	    PressKey("ralt", "num1")
	    Sleep(5)
	    ReleaseKey("ralt", "num1")
	end

	if event == "G_RELEASED" and arg == xx20 then
	    ReleaseKey("lctrl")
	    Sleep(5)
	    PressKey("ralt", "num1")
	    Sleep(5)
	    ReleaseKey("ralt", "num1")
	end

	if event == "MOUSE_BUTTON_PRESSED" and arg == 995 then
	    PressKey("lshift", "5")
	    Sleep(25)
	    ReleaseKey("lshift", "5")
	end

	if event == "MOUSE_BUTTON_PRESSED" and arg == 997 then
	    PressKey("lshift", "6")
	    Sleep(25)
	    ReleaseKey("lshift", "6")
	end

	if event == "MOUSE_BUTTON_RELEASED" and arg == 995 then
	    PressKey("lshift", "1")
	    Sleep(25)
	    ReleaseKey("lshift", "1")
	end

	if event == "MOUSE_BUTTON_RELEASED" and arg == 997 then
	    PressKey("lshift", "1")
	    Sleep(25)
	    ReleaseKey("lshift", "1")
	end
end</script>
