	end

	if event == "G_RELEASED" and arg == 15 then
	    ReleaseKey("lshift")
	    Sleep(5)
	    PressKey("ralt", "num1")
	    Sleep(5)
	    ReleaseKey("ralt", "num1")
	end

	if event == "G_RELEASED" and arg == 20 then
